{
  "name": "@sterling/iq-6900",
  "version": "0.0.1",
  "description": "A Solana-based utility for retrieving and processing on-chain data from IQ6900 (ALPHA)",
  "type": "module",
  "main": "./dist/mod.js",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "pnpm run clean",
    "prepare": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts src/",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "solana",
    "blockchain",
    "iq-6900",
    "on-chain",
    "data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sterling/iq-6900"
  },
  "author": "Sterling <sterling@example.com>",
  "license": "ISC",
  "dependencies": {
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.100",
    "@types/node": "^22.13.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.8.2",
    "vitest": "^0.34.6"
  },
  "packageManager": "pnpm@8.15.3",
  "engines": {
    "node": ">=16"
  }
}
